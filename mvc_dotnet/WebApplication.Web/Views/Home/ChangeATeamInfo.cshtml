@model Team
@{
    ViewData["Title"] = "ModifyTeams";
}

<h2>Modify Teams</h2>

<p>Select which team you would like to modify</p>

<div class="form-group">
    <label asp-for="Name" class="control-label">Team Name</label>
    <select asp-for="Name" asp-items="Model.DropDownListTeam"></select>
</div>