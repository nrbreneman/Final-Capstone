@model List<Team>
@{
    ViewData["Title"] = "UserHomePage";
}

<h2>User Home Page</h2>

@*@TempData["Added"]*@

<div>
    <a asp-action="ViewMyLeague" asp-controller="Home">View All Teams in Your League</a>
    <a asp-action="ChangeMyTeamInfo" asp-controller="Home">Update Team</a>
    <a asp-action="UpdateUserInfo" asp-controller="Home">Update My Username/Password</a>
    <a asp-action="Schedule" asp-controller="Home">See Schedule</a>
    @*finalized schedule needs to be able to filter teams and propose/reject games*@
    <a asp-action="AddAvailability" asp-controller="Home">Add Availability</a>
    <a asp-action="Messages" asp-controller="Home">Message Other Teams/See Messages</a>
</div>