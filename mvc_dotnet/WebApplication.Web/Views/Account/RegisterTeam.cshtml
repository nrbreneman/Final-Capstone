@model Team
@{
    ViewData["Title"] = "RegisterTeam";
}

<h2>Register Team</h2>

<form asp-controller="account" asp-action="RegisterTeam" method="post">
    <div class="form-group">
        <label asp-for="Name"></label>
        <input type="text" asp-for="Name" />
        <span asp-validation-for="Name"></span>
    </div>
    <div class="form-group">
        <label asp-for="Org"></label>
        <input type="text" asp-for="Org" />
        <span asp-validation-for="Org"></span>
    </div>
    <div class="form-group">
        <label asp-for="League"></label>
        <input type="text" asp-for="League" />
        <span asp-validation-for="League"></span>
    </div>
    <div class="form-group">
        <label asp-for="PrimaryVenue"></label>
        <input type="text" asp-for="PrimaryVenue" />
        <span asp-validation-for="PrimaryVenue"></span>
    </div>
    <div class="form-group">
        <label asp-for="SecondaryVenue"></label>
        <input type="text" asp-for="SecondaryVenue" />
        <span asp-validation-for="SecondaryVenue"></span>
    </div>

    <div class="form-group">
        <label asp-for="HomeDates"></label>
        <input type="text" asp-for="HomeDates" />
        @*<span asp-validation-for="HomeDates"></span>*@
    </div>
    <div class="form-group">
        <label asp-for="TravelDates"></label>
        <input type="text" asp-for="TravelDates" />
        @*<span asp-validation-for="TravelDates"></span>*@
    </div>
    

    <input type="submit" value="RegisterTeam" class="btn btn-default" />

</form>